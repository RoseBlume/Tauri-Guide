{
  "productName": "Tauri-Guide",
  "version": "0.1.0",
  "identifier": "io.github.roseblume.tauriguide",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Tauri-Guide",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "android": {
      "minSdkVersion": 24
    }
  }
}
